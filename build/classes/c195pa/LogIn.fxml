<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.LogInController">
   <children>
      <VBox layoutX="77.0" layoutY="98.0" prefHeight="262.0" prefWidth="260.0">
         <children>
            <Label fx:id="UsernameLbl" text="Username:">
               <padding>
                  <Insets bottom="15.0" top="15.0" />
               </padding>
            </Label>
            <TextField fx:id="UsernameTxt" prefHeight="14.0" prefWidth="196.0" />
            <Label fx:id="PasswordLbl" text="Password:">
               <padding>
                  <Insets bottom="15.0" top="15.0" />
               </padding>
            </Label>
            <TextField fx:id="PasswordTXT" />
            <Button fx:id="SubmitBttn" alignment="CENTER" mnemonicParsing="false" onAction="#onActionSubmitClick" prefHeight="32.0" prefWidth="80.0" text="Submit">
               <VBox.margin>
                  <Insets left="80.0" top="40.0" />
               </VBox.margin>
            </Button>
         </children>
      </VBox>
      <HBox layoutX="88.0" layoutY="23.0" prefHeight="65.0" prefWidth="237.0">
         <children>
            <Label fx:id="WelcomeLbl" alignment="CENTER" prefHeight="25.0" prefWidth="235.0" text="Welcome! Please Log-in" textAlignment="CENTER" underline="true">
               <font>
                  <Font name="System Bold" size="18.0" />
               </font>
            </Label>
         </children>
      </HBox>
   </children>
</AnchorPane>
