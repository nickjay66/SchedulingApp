<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="427.0" prefWidth="233.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.ChooseAppointmentController">
   <children>
      <HBox layoutY="48.0" prefHeight="35.0" prefWidth="270.0">
         <children>
            <Label text="Choose a Appointment to Modify" textAlignment="CENTER">
               <font>
                  <Font name="Lucida Bright Regular" size="14.0" />
               </font>
               <HBox.margin>
                  <Insets left="19.5" />
               </HBox.margin>
            </Label>
         </children>
      </HBox>
      <HBox layoutX="-2.0" layoutY="386.0" prefHeight="100.0" prefWidth="259.0">
         <children>
            <Button fx:id="onActionSubmit" mnemonicParsing="false" onAction="#onActionSubmit" prefHeight="35.0" text="Submit">
               <HBox.margin>
                  <Insets left="65.5" />
               </HBox.margin>
            </Button>
            <Button mnemonicParsing="false" onAction="#onActionCancel" prefHeight="35.0" text="Cancel">
               <HBox.margin>
                  <Insets left="15.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
      <TableView fx:id="AppointmentTable" layoutX="24.0" layoutY="128.0" prefHeight="230.0" prefWidth="223.0">
         <columns>
            <TableColumn fx:id="appIdCol" prefWidth="110.0" text="App. Id" />
            <TableColumn fx:id="custNameCol" prefWidth="112.0" text="Customer" />
         </columns>
      </TableView>
   </children>
</AnchorPane>
